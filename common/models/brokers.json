{
  "name": "Broker",
  "description": "A model represting a broker",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "bdr": {
      "type": "object",
      "required": true
    },
    "name": {
      "type": "string",
      "required": false
    },
    "brokerage": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "policies": {
      "type": "hasAndBelongsToMany",
      "model": "policy",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__findById__accessTokens"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "greet"
    }
  ],
  "methods": {}
}
