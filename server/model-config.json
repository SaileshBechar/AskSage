{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "Broker": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create":true,
          "login": true,
          "logout": true,
          "prototype.__findById__accessTokens": true,
          "findById": true,
          "greet": true
          
        }
      }
    }
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "feedback": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "create": true,
          "find": true,
          "findById": true
        }
      }
    }
  },
  "news": {
    "dataSource": "newsAPI",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getNews": true
        }
      }
    }
  },



 "swapi": {
    "dataSource": "swapi",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true
         
        }
      }
    }
  }



}
